<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.cym.mapper.CRootMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.example.cym.pojo.CRoot">
        <id column="id" property="id" />
        <result column="password" property="password" />
        <result column="slat" property="slat" />
        <result column="start_login" property="startLogin" />
        <result column="end_login" property="endLogin" />
        <result column="login_count" property="loginCount" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, password, slat, start_login, end_login, login_count
    </sql>

</mapper>
